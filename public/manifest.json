{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/refs/heads/master/src/schemas/json/web-manifest.json",
  "name": "BlueCatalog",
  "description": "Search through your Bluesky likes and bookmarks easily",
  "start_url": "/",
  "display": "minimal-ui",
  "icons": [
    {
      "src": "/icons/icon.svg",
      "sizes": "256x256",
      "type": "image/svg",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "any"
    }
  ]
}
